@model List<ZkConstruction.ViewModelQuery.WorksheetVMQ>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="content-wrapper">

    <!-- Container-fluid starts -->
    <!-- Main content starts -->
    <div class="container-fluid">
        <div class="row">
            <div class="main-header">
            </div>
            <div id="mydiv" class="col-md-3">
                <p class="bg-success text-center">@TempData["update"]</p>
                <p class="bg-danger text-center">@TempData["Delete"]</p>
            </div>
        </div>
        <div class="col-lg-12">
            <a asp-action="Create" asp-controller="WorkSheet" class="btn btn-skype offset-lg-10"><i class="fa fa-plus"></i>Create New</a>
            <div class="card" style="margin-top:20px">
                <div class="row">
                    <div class="col-sm-12">
                        <div class="card">
                            <div class="card-header">
                                <h5 class="card-header-text"><u>Work Sheets Tables</u></h5>
                            </div>
                            <div class="card-block">
                                <table id="table1" class="table table-striped table-bordered" style="overflow-x:auto">
                                    <thead>
                                        <tr>
                                            <th>Provider Order No</th>
                                            <th>Accoumadtion No</th>
                                            <th>Project Name</th>
                                            <th>Site Name</th>
                                            <th>Date</th>
                                            <th>Company Name</th>
                                            <th>Site Address</th>
                                            <th>Floor/Side/PAV</th>
                                        </tr>
                                    </thead>

                                    <tbody>
                                    @foreach (var lst in Model) 
                                    {
                                    <tr>
                                        <td>@lst.ProviderOrderNo</td>
                                        <td>@lst.AccoummodationNo</td>
                                        <td>@lst.ProjectName</td>
                                        <td>@lst.SiteName</td>
                                        <td>@lst.Date</td>
                                        <td>@lst.CompanyName</td>
                                        <td>@lst.SiteAddress</td>
                                        <td>@lst.FloorSide</td>
                                        @*<td>
                                            <a href="@Url.Action("Edit", new { id = lst.id })" class="badge badge-primary" data-toggle="tooltip" data-placement="left" title="Click Me To Edit"><i class="fa fa-pencil"></i></a>
                                        </td>*@
                                    </tr>
                                    }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Main content ends -->
    <!-- Container-fluid ends -->
</div>
<script>
    $('.chosen-select').chosen();
    setTimeout(function () {
        $('#mydiv').fadeOut('fast');
    }, 3000); // <-- time in milliseconds
    //start
    $(document).ready(function () {
        var table1 = $('#table1').DataTable({
            "paging": true,
            "ordering": true,
            "info": true,
        });
    });
</script>