                                <tr>
                                    <td><a data-toggle="modal" data-target="#com-@Lst.Proid">@Lst.CompanyName.ToUpper()</a></td>
                                    <td><a data-toggle="modal" data-target="#Sr-@Lst.Proid">@Lst.Sr</a></td>
                                    <td><a data-toggle="modal" data-target="#Site-@Lst.Proid"> @Lst.SiteName</a></td>
                                    <td><a data-toggle="modal" data-target="#Floor-@Lst.Proid">@Lst.Floor </a></td>
                                    <td><a data-toggle="modal" data-target="#Cus-@Lst.Proid">@Lst.CustomerName.ToUpper()</a></td>
                                    <td><a data-toggle="modal" data-target="#Work-@Lst.Proid">@Lst.WorkHours</a></td>
                                    <td><a data-toggle="modal" data-target="#Hand-@Lst.Proid">@Convert.ToDateTime(Lst.HandDate).ToString("dd/MM/yyyy")</a></td>
                                    <td>
                                        @if (Lst.Pa == "Yes")
                                            {
                                        <a data-toggle="modal" data-target="#Paint-@Lst.Proid"><span style="color:limegreen"><b>@Lst.Pa</b></span></a>
                                            }
                                            else
                                            {
                                        <a data-toggle="modal" data-target="#Paint-@Lst.Proid"><span>No</span></a>
                                            }
                                    </td>
                                    <td>
                                        <a data-toggle="modal" data-target="#Flooring-@Lst.Proid">
                                            @if (Lst.Fl == "Yes")
                                                {
                                            <span style="color:limegreen"><b>@Lst.Fl</b></span>
                                                }
                                                else
                                                {
                                            <span>No</span>
                                                }
                                        </a>
                                    </td>
                                    <td>
                                        <a data-toggle="modal" data-target="#Wallpaper-@Lst.Proid">
