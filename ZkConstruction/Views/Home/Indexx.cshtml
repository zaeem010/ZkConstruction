                                    <th>Company</th>
                                    <th>Sr#</th>
                                    <th>Address</th>
                                    <th>Floor</th>
                                    <th>Customer</th>
                                    <th>Work Hours</th>
                                    <th>Ending Date</th>
                                    <th>Paint</th>
                                    <th>Flooring</th>
                                    <th>Wallpaper</th>
                                    <th>Cleaning</th>
                                    <th>Manager</th>
                                    <th>Painting Employee</th>
                                    <th>Painting Hours</th>
                                    <th>Start Date Time</th>
                                    <th>Check Out</th>
                                    <th>Electricity</th>
                                    <th>Flooring Employee</th>
                                    <th>Flooring Hours</th>
                                    <th>Start Date Time</th>
                                    <th>Check Out</th>
                                    <th>Flooring Room</th>
                                    <th>Remaining Hours</th>
                                    <th>Starting Date</th>
                                </tr>
                            </thead>

                            <tfoot>
                                <tr>
                                    <th>Company</th>
                                    <th>Sr#</th>
                                    <th>Address</th>
                                    <th>Floor</th>
                                    <th>Customer</th>
                                    <th>Work Hours</th>
                                    <th>Ending Date</th>
                                    <th>Paint</th>
                                    <th>Flooring</th>
                                    <th>Wallpaper</th>
                                    <th>Cleaning</th>
                                    <th>Manager</th>
                                    <th>Painting Employee</th>
                                    <th>Painting Hours</th>
                                    <th>Start Date Time</th>
                                    <th>Check Out</th>
                                    <th>Electricity</th>
                                    <th>Flooring Employee</th>
                                    <th>Flooring Hours</th>
                                    <th>Start Date Time</th>
                                    <th>Check Out</th>
                                    <th>Flooring Room</th>
                                    <th>Remaining Hours</th>
                                    <th>Starting Date</th>
                                </tr>
                            </tfoot>
