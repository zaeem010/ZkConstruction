                                    <td><a data-toggle="modal" data-target="#PaintHours-@Lst.Proid"> @Lst.PaintingHours </a></td>
                                    <td><a data-toggle="modal" data-target="#StTime-@Lst.Proid"> @Convert.ToDateTime(Lst.StTime).ToString("dd/MM/yyyy HH:mm")</a></td>
                                    <td><a data-toggle="modal" data-target="#PaClosing-@Lst.Proid"><span class="badge badge-primary"><i class="fa fa-eye"></i></span></a></td>
                                    <td><a data-toggle="modal" data-target="#El-@Lst.Proid">@Lst.Electricity</a></td>
